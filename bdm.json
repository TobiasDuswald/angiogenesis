    },
    "visualize_diffusion": [
      {
        "_typename": "bdm::Param::VisualizeDiffusion",
        "concentration": true,
        "gradient": true,
        "name": "VEGF"
      }
    ]
  },
  "bdm::SimParam": {
    "_typename": "bdm::SimParam",
    "total_sim_time": 1200,
    "visualization_interval": 5,
    "default_vessel_length": 30,
    "secretion_rate_vegf": 0.03,
    "diffusion_vegf": 2.0
  }
